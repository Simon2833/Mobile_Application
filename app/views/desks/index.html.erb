  
  <h1>Desks</h1>
  <table class="table table-striped table-bordered table-hover">

    <thead class="thead-dark">
      <tr>
      <th>id</th>
      <th>Status</th>
      </tr>
    </thead>

    <tbody>
      <% @desks.each do |desk| %>
        <tr>

          <td><%= desk.id %></td>
        
          <td><%= desk.status %></td>


          <td><%= link_to 'Show', desk %></td>
          <td><%= link_to 'Edit', edit_desk_path(desk) %></td>
          <td><%= link_to 'Delete', desk, method: "delete", data: { confirm: 'Are you sure?' }, class: "btn btn-outline-danger btn-sm" %></td>

        </tr>


        <% end %>

    </tbody>
  </table>


  <br>

  <%= link_to 'New Desk', new_desk_path, class:"btn btn-dark" %>

